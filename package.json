{
  "private": true,
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "jest": "*"
  },
  "scripts": {
    "test": "jest",
    "watch": "jest --watch"
  },
  "babel": {
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ],
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "9.2.1",
            "chrome": "63"
          },
          "exclude": [
            "transform-regenerator",
            "transform-async-to-generator"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "lodash": "^4.17.4"
  }
}
